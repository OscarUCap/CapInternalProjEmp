@model Employee

@{
    ViewBag.Title = "Delete Confirm";
}

<h3>
    Are you sure you want to delete this Employee?
</h3>

<br>

<div class="btn-group">
    <a asp-controller="Employee" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Yes</a>
    <a asp-controller="Employee" asp-action="List" class="btn btn-success">No</a>
</div>